<template id="maponline_etiqueta" name="maponline">
    <t t-name="js_maponline">
        <div style="position:relative; width:100%; height:100%;">                        
            <div id="menu_vehicles" style="width:250px; height: calc(100% - 70px); position:absolute; top:0px; left:0px; display: block;"/>
            <div id="maponline" style="top:0px; left:250px; width: calc(100% - 250px); height:100%;"/>  
            <div id="tableros" class="o_main_navbar" style="z-index:100; font-size:25px; text-align:left; position:absolute; bottom:0px; left:0px; width:100%; height:70px; color:#ccc; padding:10px;"/>            <t t-call="gpsmaps_odometers"/>
        </div>
    </t>
    <t t-name="div_map">
        <div style="position:relative; width:100%; height:100%; display:block; background-color:#ccc;">                        
            <div id="maponline" style="top:0px; left:0px; width:100%; height:400px;"/>  
        </div>
    </t>    
</template>
